<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
<bean class="com.First.Laptop" id="l1">
<!-- You can not miss-match the value while assigning  -->
<constructor-arg value="Vivobook"></constructor-arg>
<constructor-arg value="Asus"></constructor-arg>
<constructor-arg value="60000"></constructor-arg>
<constructor-arg ref="p1"></constructor-arg>
</bean>    
   
   
<bean class="com.First.Processor" id="p1">
<constructor-arg value="Intel"></constructor-arg>
<constructor-arg value="5"></constructor-arg>
</bean>

</beans>